                    "surname": {
                        "type": "string",
                        "description": "Surname of the User",
                        "minLength": 3,
                        "maxLength": 100
                    },
                    "expectedCaloriesPerDay": {
                        "type": "number",
                        "description": "Expected daily intake of calories per day for the User"
                    }
                },
                "additionalProperties": false,
                "required": [
                    "email",
                    "surname",
                    "expectedCaloriesPerDay",
                    "name"
                ]
