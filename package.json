{
  "name": "caloriette",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/tsc --p .",
    "prebuild": "rm -rf ./dst && npm run generate-code",
    "prestart": "npm run build",
    "start": "node dst/index.js",
    "postbuild": "npm run lint",
    "generate-code": "sh ./schema/scripts/compile.sh",
    "pretest": "npm run build",
    "test": "npm run mocha",
    "lint": "./node_modules/.bin/tslint -p ./tsconfig.json -c ./tslint.json --fix",
    "mocha": "./node_modules/.bin/nyc --extension=.ts --reporter=lcov --reporter=text-summary ./node_modules/.bin/mocha --recursive ./dst/test --bail --sort --timeout 10000"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 50,
    "statements": 50,
    "functions": 40,
    "exclude": [
      "**/test"
    ],
    "watermarks": {
      "branches": [
        10,
        50
      ],
      "lines": [
        90,
        100
      ],
      "functions": [
        90,
        100
      ],
      "statements": [
        90,
        100
      ]
    }
  },
  "devDependencies": {
    "@types/chai": "^4.2.9",
    "@types/express": "^4.17.2",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/mocha": "^5.2.6",
    "@types/mock-require": "^2.0.0",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.1.4",
    "@types/uuid": "^3.4.4",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "mock-require": "^3.0.3",
    "nyc": "^14.1.0",
    "tslint": "^5.11.0",
    "typeorm": "^0.2.22",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.3.0",
    "http-status-codes": "^1.3.2",
    "ajv": "^6.10.0",
    "morgan": "^1.9.0",
    "uuid": "^3.2.2"
  },
  "author": "",
  "license": "ISC"
}
